class stack:
    def __init__(self):
        self.list=[]
    
    def __str__(self):
        values=self.list.reverse()
        values=[str(x) for x in self.list]
        return '\n'.join(values)
    
    def isempty(self):
        if self.list == []:
            return True
        else:
            return False
            
    def push(self,value):
        self.list.append(value)
        return 'element inserted successfully'
        
    def pop(self):
        if self.isempty():
            return 'no element'
        else:
            return self.list.pop()
    
    def peek(self):
        if self.isempty():
            return 'no element'
        else:
            return self.list[len(self.list)-1]
            
    
    
    
cs=stack()
print(cs.isempty())
cs.push(1)
cs.push(2)
cs.push(3)
#cs.pop()
#print(cs)
print(cs.peek())
